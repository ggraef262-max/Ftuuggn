-- Ù…ØªØºÙŠØ±Ø§Øª
local names = {"", "", ""}
local execSpeed = 1
local toggleActive = false

-- Ø¥Ù†Ø´Ø§Ø¡ TextBox Ù„Ù„Ø£Ø³Ù…Ø§Ø¡ Ø§Ù„Ø«Ù„Ø§Ø«Ø©
for i = 1, 3 do
Tab:AddTextBox({
        Name = "Ø£Ø¯Ø®Ù„ Ø§Ù„Ø§Ø³Ù… " .. i,
        Default = "",
        TextDisappear = false,
        Callback = function(value)
            names[i] = value
        end
    })
end

-- Slider Ù„ØªØ­Ø¯ÙŠØ¯ Ø§Ù„Ø³Ø±Ø¹Ø©
Tab:AddSlider({
    Name = "Ø³Ø±Ø¹Ø© Ø§Ù„Ø³Ø¨Ø§Ù…",
    Min = 1,
    Max = 3,
    Default = 1,
    Increment = 1,
    ValueName = "x",
    Callback = function(value)
        execSpeed = value
    end
})

-- Toggle Ù„Ù„ØªØ´ØºÙŠÙ„ Ù…Ø¹ Ø§Ù„Ø­Ù…Ø§ÙŠØ©
Tab:AddToggle({
    Name = "ØªØ´ØºÙŠÙ„ Ø§Ù„Ø³Ø¨Ø§Ù…",
    Default = false,
    Callback = function(state)
        toggleActive = state
        if toggleActive then
            -- ØªØ­Ù‚Ù‚ Ø£Ù† ÙÙŠÙ‡ Ø¹Ù„Ù‰ Ø§Ù„Ø£Ù‚Ù„ Ø§Ø³Ù… ÙˆØ§Ø­Ø¯ Ù…ÙƒØªÙˆØ¨
            local hasName = false
            for _, name in ipairs(names) do
                if name ~= "" then
                    hasName = true
                    break
                end
            end

            if not hasName then
                game:GetService("StarterGui"):SetCore("SendNotification", {
                    Title = "ØªØ­Ø°ÙŠØ±",
                    Text = "ÙŠØ¬Ø¨ Ø¥Ø¯Ø®Ø§Ù„ Ø§Ø³Ù… ÙˆØ§Ø­Ø¯ Ø¹Ù„Ù‰ Ø§Ù„Ø£Ù‚Ù„ Ù‚Ø¨Ù„ Ø§Ù„ØªØ´ØºÙŠÙ„"
                })
                toggleActive = false
                return
            end

            -- Ø¨Ø¯Ø¡ Ø§Ù„ØªØ´ØºÙŠÙ„
            task.spawn(function()
                while toggleActive do
                    for _, name in ipairs(names) do
                        if not toggleActive then break end
                        if name ~= "" then
                            local args = {
                                "RolePlayName",
                                name
                            }
                            game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1RPNam1eTex1t"):FireServer(unpack(args))
                            task.wait((4 - execSpeed) * 0.5)
                        end
                    end
                end
            end)
        end
    end
})

