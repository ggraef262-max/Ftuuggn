local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local CoreGui = game:GetService("CoreGui")
local SoundService = game:GetService("SoundService")
local Lighting = game:GetService("Lighting")
local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local UserInputService = game:GetService("UserInputService")

-- إخفاء الإشعارات
StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Chat, false)
StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, false)
StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.PlayerList, false)

-- ====== الألوان الفنية ======
local GOLD = Color3.fromRGB(184, 115, 51)  -- ذهبي
local DARK_GOLD = Color3.fromRGB(120, 70, 20)  -- ذهبي غامق
local FIRE = Color3.fromRGB(255, 100, 0)  -- ناري
local BLOOD = Color3.fromRGB(180, 40, 40)  -- دم

-- ====== مؤثرات سينمائية ======
local blur = Instance.new("BlurEffect", Lighting)
blur.Size = 20

local colorCorrection = Instance.new("ColorCorrectionEffect", Lighting)
colorCorrection.TintColor = GOLD
colorCorrection.Contrast = 0.2
colorCorrection.Saturation = 0.1

-- ====== موسيقى تصويرية ======
local music = Instance.new("Sound", SoundService)
music.SoundId = "rbxassetid://1838138986"  -- موسيقى حماسية
music.Volume = 0.3
music.Looped = true
music.PlaybackSpeed = 0.9
music:Play()

-- ====== الواجهة الفنية ======
local gui = Instance.new("ScreenGui", CoreGui)
gui.Name = "ArtLoader"
gui.IgnoreGuiInset = true
gui.ResetOnSpawn = false
gui.DisplayOrder = 999

-- ====== الصورة الفنية ======
local artImage = Instance.new("ImageLabel", gui)
artImage.Size = UDim2.fromScale(1, 1)
artImage.Position = UDim2.fromScale(0.5, 0.5)
artImage.AnchorPoint = Vector2.new(0.5, 0.5)
artImage.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
artImage.Image = "rbxassetid://126865658241127"  -- صورتك الفنية
artImage.ImageColor3 = GOLD
artImage.ImageTransparency = 0.15
artImage.ScaleType = Enum.ScaleType.Crop
artImage.ZIndex = 0

-- ====== تأثير الزجاج ======
local glassEffect = Instance.new("Frame", gui)
glassEffect.Size = UDim2.fromScale(1, 1)
glassEffect.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
glassEffect.BackgroundTransparency = 0.3
glassEffect.BorderSizePixel = 0
glassEffect.ZIndex = 1

-- ====== إطار فني ======
local topBorder = Instance.new("Frame", gui)
topBorder.Size = UDim2.new(1, 0, 0, 3)
topBorder.Position = UDim2.fromScale(0, 0)
topBorder.BackgroundColor3 = GOLD
topBorder.BorderSizePixel = 0
topBorder.ZIndex = 5

local bottomBorder = Instance.new("Frame", gui)
bottomBorder.Size = UDim2.new(1, 0, 0, 3)
bottomBorder.Position = UDim2.fromScale(0, 1)
bottomBorder.AnchorPoint = Vector2.new(0, 1)
bottomBorder.BackgroundColor3 = GOLD
bottomBorder.BorderSizePixel = 0
bottomBorder.ZIndex = 5

-- ====== اسم فني ======
local title = Instance.new("TextLabel", gui)
title.Size = UDim2.fromScale(0.9, 0.2)
title.Position = UDim2.fromScale(0.5, 0.2)
title.AnchorPoint = Vector2.new(0.5, 0.5)
title.BackgroundTransparency = 1
title.Text = "CLOCK 7"
title.Font = Enum.Font.GothamBlack
title.TextSize = 80
title.TextColor3 = GOLD
title.TextStrokeTransparency = 0.2
title.TextStrokeColor3 = FIRE
title.ZIndex = 4

local subTitle = Instance.new("TextLabel", gui)
subTitle.Size = UDim2.fromScale(0.5, 0.05)
subTitle.Position = UDim2.fromScale(0.5, 0.3)
subTitle.AnchorPoint = Vector2.new(0.5, 0.5)
subTitle.BackgroundTransparency = 1
subTitle.Text = "ART COLLECTION"
subTitle.Font = Enum.Font.Gotham
subTitle.TextSize = 24
subTitle.TextColor3 = GOLD
subTitle.TextTransparency = 0.4
subTitle.ZIndex = 4

-- ====== شريط فني ======
local progressBarBg = Instance.new("Frame", gui)
progressBarBg.Size = UDim2.fromScale(0.7, 0.04)
progressBarBg.Position = UDim2.fromScale(0.5, 0.6)
progressBarBg.AnchorPoint = Vector2.new(0.5, 0.5)
progressBarBg.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
progressBarBg.BorderSizePixel = 0
progressBarBg.ZIndex = 3

local progressBarCorner = Instance.new("UICorner", progressBarBg)
progressBarCorner.CornerRadius = UDim.new(1, 0)

-- شريط متدرج
local progressBar = Instance.new("Frame", progressBarBg)
progressBar.Size = UDim2.fromScale(0, 1)
progressBar.BackgroundColor3 = GOLD
progressBar.BorderSizePixel = 0
progressBar.ZIndex = 3

local progressGradient = Instance.new("UIGradient", progressBar)
progressGradient.Color = ColorSequence.new({
    ColorSequenceKeypoint.new(0, GOLD),
    ColorSequenceKeypoint.new(0.5, FIRE),
    ColorSequenceKeypoint.new(1, BLOOD)
})

local progressBarCorner2 = Instance.new("UICorner", progressBar)
progressBarCorner2.CornerRadius = UDim.new(1, 0)

-- ====== نسبة فنية ======
local percentage = Instance.new("TextLabel", gui)
percentage.Size = UDim2.fromScale(0.3, 0.06)
percentage.Position = UDim2.fromScale(0.5, 0.68)
percentage.AnchorPoint = Vector2.new(0.5, 0.5)
percentage.BackgroundTransparency = 1
percentage.Text = "0%"
percentage.Font = Enum.Font.GothamBold
percentage.TextSize = 36
percentage.TextColor3 = GOLD
percentage.TextStrokeTransparency = 0.5
percentage.TextStrokeColor3 = FIRE
percentage.ZIndex = 3

-- ====== تأثيرات بصرية ======
local particles = {}
for i = 1, 15 do
    local particle = Instance.new("Frame", gui)
    particle.Size = UDim2.fromOffset(math.random(2, 6), math.random(2, 6))
    particle.Position = UDim2.fromScale(math.random(), math.random())
    particle.BackgroundColor3 = GOLD:lerp(FIRE, math.random())
    particle.BackgroundTransparency = 0.3
    particle.BorderSizePixel = 0
    particle.ZIndex = 2
    
    local particleCorner = Instance.new("UICorner", particle)
    particleCorner.CornerRadius = UDim.new(1, 0)
    
    table.insert(particles, {
        frame = particle,
        speed = Vector2.new(math.random(-30, 30)/1000, math.random(-20, -5)/1000),
        transparencySpeed = math.random(1, 3)/10
    })
end

-- ====== حركة فنية ======
RunService.RenderStepped:Connect(function(dt)
    local time = tick()
    
    -- حركة الجسيمات
    for _, p in ipairs(particles) do
        local pos = p.frame.Position
        local newX = pos.X.Scale + p.speed.X * dt * 30
        local newY = pos.Y.Scale + p.speed.Y * dt * 30
        
        if newY < 0 then
            newY = 1
            newX = math.random()
        end
        
        p.frame.Position = UDim2.fromScale(newX, newY)
        p.frame.BackgroundTransparency = 0.3 + math.sin(time * p.transparencySpeed) * 0.2
    end
    
    -- نبض خفيف للعنوان
    title.TextTransparency = 0.1 + math.sin(time * 3) * 0.1
    title.TextStrokeTransparency = 0.2 + math.sin(time * 4) * 0.1
end)

-- ====== تحميل فني ======
task.wait(0.5)

-- تأثير دخول
TweenService:Create(title, TweenInfo.new(1, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
    TextTransparency = 0,
    Position = UDim2.fromScale(0.5, 0.2)
}):Play()

-- تحميل السكربت
local success, result = pcall(function()
    return game:HttpGet("https://raw.githubusercontent.com/ggraef262-max/Ftuuggn/refs/heads/main/Script")
end)

if success and result then
    for i = 1, 100 do
        -- تحديث الشريط
        progressBar.Size = UDim2.fromScale(i/100, 1)
        percentage.Text = i .. "%"
        
        -- تأثيرات فنية أثناء التحميل
        if i % 25 == 0 then
            -- نبضة عند كل 25%
            TweenService:Create(progressBar, TweenInfo.new(0.2), {
                BackgroundColor3 = FIRE
            }):Play()
            task.wait(0.1)
            TweenService:Create(progressBar, TweenInfo.new(0.2), {
                BackgroundColor3 = GOLD
            }):Play()
        end
        
        task.wait(0.01)
    end
    
    percentage.Text = "✓ COMPLETE"
    
    -- صوت نجاح
    local successSound = Instance.new("Sound", SoundService)
    successSound.SoundId = "rbxassetid://9120391256"
    successSound.Volume = 2
    successSound:Play()
    
    task.wait(0.5)
    
    -- تشغيل السكربت
    pcall(function() loadstring(result)() end)
else
    percentage.Text = "✗ ERROR"
    task.wait(2)
end

-- تلاشي فني
TweenService:Create(glassEffect, TweenInfo.new(1), {BackgroundTransparency = 1}):Play()
TweenService:Create(artImage, TweenInfo.new(1), {ImageTransparency = 1}):Play()
TweenService:Create(music, TweenInfo.new(2), {Volume = 0}):Play()

task.wait(1.5)

-- تنظيف
blur:Destroy()
colorCorrection:Destroy()
music:Destroy()
gui:Destroy()
gui:Destroy()
