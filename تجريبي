        if connection then
            pcall(function() connection:Disconnect() end)
        end
    end
    connections = {}
    
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "â¹ï¸ ØªÙ… Ø§Ù„Ø¥ÙŠÙ‚Ø§Ù",
        Text = "ØªÙ… Ø¥ÙŠÙ‚Ø§Ù Ø¬Ù…ÙŠØ¹ Ø§Ù„Ø­Ù…Ø§ÙŠØ§Øª",
