            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "âœ… Ù…Ø¶Ø§Ø¯ Ø³Ø¨ÙŠØ¯",
                Text = "ØªÙ… Ø§Ù„ØªØ´ØºÙŠÙ„",
                Duration = 1
            })
